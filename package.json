  "private": true,
  "scripts": {
    "analyze": "nuxt build --analyze",
    "build": "nuxt build",
    "dev": "nuxt --port 3005",
    "generate": "nuxt generate",
    "lint": "yarn lint:js",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
