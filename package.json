    "core-js": "^3.9.1",
    "firebase": "^8.6.7",
    "firebaseui": "^4.8.0",
    "nuxt": "^2.15.3",
    "nuxt-property-decorator": "^2.9.1",
    "querystring": "^0.2.1",
    "validator": "^13.6.0",
    "vue": "^2.6.12",
    "xml2js": "^0.4.23",
    "yup": "^0.32.9"
  },
  "resolutions": {
    "sass": "1.32.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
