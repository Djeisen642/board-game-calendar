    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.2",
    "eslint": "^7.22.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "ts-jest": "^26.5.4",
    "vue-eslint-parser": "^7.6.0",
    "vue-jest": "^3.0.4",
